    "coverage": "npm run test -- --coverage",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dronesitter/bf-blackbox-parser"
  },
  "author": "dronesitter <mirikle.wang@gmail.com> (https://github.com/dronesitter)",
  "license": "GPL 3.0",
  "bugs": {
    "url": "https://github.com/dronesitter/bf-blackbox-parser/issues"
  },
  "homepage": "https://github.com/dronesitter/bf-blackbox-parser",
  "keywords": [
    "library",
    "starter",
    "es6"
  ],
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-typescript": "^7.19.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.6",
    "@types/jest": "^28.1.3",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "babel-preset-minify": "^0.5.2",
    "css-loader": "^6.7.1",
    "eslint": "^7.32.0",
    "file-loader": "^6.2.0",
